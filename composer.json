{
    "name": "zareismail/ceremony",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "zareismail/markable": "~0.1",
        "zareismail/nova-location": "~0.1",
        "zareismail/nova-contracts": "*"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\Ceremony\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\Ceremony\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
